{
  "name": "@tradeit/server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
  "build": "tsc -p tsconfig.build.json",
  "dev": "tsx watch src/index.ts",
  "dev:matcher": "tsx watch src/matcherWorker.ts",
  "dev:gateway": "tsx watch src/gatewayServer.ts",
  "start": "node --experimental-specifier-resolution=node dist/index.js",
  "start:matcher": "node --experimental-specifier-resolution=node dist/matcherWorker.js",
  "start:gateway": "node --experimental-specifier-resolution=node dist/gatewayServer.js",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,json}'",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev --name init",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio"
  },
  "dependencies": {
    "@msgpack/msgpack": "^2.8.0",
    "@prisma/client": "^5.22.0",
    "@tradeit/shared": "file:../shared",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "pino": "^9.1.0",
    "pino-pretty": "^11.0.0",
    "uuid": "^9.0.1",
    "ws": "^8.17.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.19",
    "@types/ws": "^8.5.9",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-unused-imports": "^3.1.0",
    "prettier": "^3.2.5",
    "prisma": "^5.22.0",
    "tsx": "^4.7.0",
  "typescript": "^5.6.3",
    "vitest": "^1.6.0"
  }
}
